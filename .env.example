# Environment Variables for MoneyPrinterPlus
# Copy this file to .env and fill in your actual API keys
# DO NOT commit .env to version control

# Audio Services - Ali
ALI_ACCESS_KEY_ID=your_ali_access_key_id_here
ALI_ACCESS_KEY_SECRET=your_ali_access_key_secret_here
ALI_APP_KEY=your_ali_app_key_here

# Audio Services - Tencent
TENCENT_ACCESS_KEY_ID=your_tencent_access_key_id_here
TENCENT_ACCESS_KEY_SECRET=your_tencent_access_key_secret_here
TENCENT_APP_KEY=your_tencent_app_key_here

# Audio Services - Azure
AZURE_SERVICE_REGION=your_azure_service_region_here
AZURE_SPEECH_KEY=your_azure_speech_key_here

# LLM Services
DEEPSEEK_API_KEY=your_deepseek_api_key_here
